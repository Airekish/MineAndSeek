#Display spectating messagetitle @s title ["",{"text":"SPECTATING","bold":true,"italic":true,"color":"white"}]#Show death message in chat if died in-gameexecute unless score @s deathswap.deaths matches 100 run tellraw @s ["",{"text":"[","bold":true,"color":"gray"},{"text":"DeathSwap","bold":true,"color":"blue"},{"text":"]","bold":true,"color":"gray"},{"text":" You were killed!"}]#Show spectating message in chat if joined mid-gameexecute if score @s deathswap.deaths matches 100 run tellraw @s ["",{"text":"[","bold":true,"color":"gray"},{"text":"DeathSwap","bold":true,"color":"blue"},{"text":"]","bold":true,"color":"gray"},{"text":" You are spectating since you joined mid-game."}]